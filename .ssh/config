Include config.d/*.conf

Host athos
    ForwardAgent yes

Host athos_ext
    HostName athos
    ForwardAgent yes
#   ProxyJump lantea
    ProxyCommand ssh lantea -W %h:%p

Host halo
    HostName halo.minimum.se
    Port 2221
    ForwardAgent yes

Host prime
    HostName minimum.se
    ForwardAgent yes

Host hellbay
    HostName hellbay.minimum.se
    Port 2222
    ForwardAgent yes

Host lantea
    HostName b.mag32.net
    ForwardAgent yes

Host rp
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    HostName localhost
    Port 8080
#    ProxyJump lantea
    ProxyCommand ssh lantea -W %h:%p

Host lnxlaptop
    HostName 10.161.4.101
    ProxyCommand ssh rp -W %h:%p

Host ilo
    HostName ilo
    KexAlgorithms +diffie-hellman-group1-sha1
    Ciphers aes128-ctr,aes192-ctr,aes256-ctr,aes128-cbc,3des-cbc
    HostKeyAlgorithms +ssh-rsa
